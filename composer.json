{
  "name": "sioweb/oxid-api",
  "description": "Symfony based api",
  "type": "oxideshop-module",
  "keywords": [
    "oxid",
    "modules",
    "cronjob"
  ],
  "homepage": "https://www.sioweb.de",
  "license": [
    "GPL-3.0-only"
  ],
  "extra": {
    "oxideshop": {
      "blacklist-filter": [
        "documentation/**/*.*"
      ],
      "source-directory": "./src/Resources/oxid",
      "target-directory": "sioweb/Api"
    },
    "oxid-kernel-plugin": "Sioweb\\Oxid\\Api\\SiowebOxidApiBundle"
  },
  "require": {
    "php": ">=7.0",
    "sioweb/oxid-kernel": "*",
    "symfony/security": "^2",
    "symfony/security-bundle": "^2"
  },
  "autoload": {
    "psr-4": {
      "Sioweb\\Oxid\\Api\\Legacy\\": "../../../source/modules/sioweb/Api/",
      "Sioweb\\Oxid\\Api\\": "src/"
    },
    "exclude-from-classmap": [
        "src/Resources/oxid"
    ]
  }
}
